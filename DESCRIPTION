Package: AMAPVox
Type: Package
Title: LiDAR Data Voxelisation
Version: 0.3.0
Authors@R: c(
  person("Gr√©goire", "Vincent", email="gregoire.vincent@ird.fr", role=c("aut")),
  person("Julien", "Heurtebize", email="julienhtbe@gmail.com", role=c("aut")),
